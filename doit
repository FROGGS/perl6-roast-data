nice -20 ./spec_p
nice -20 ./spec_n
nice -20 ./spec_r
TEMPFILE="TEMPFILE"
PASSFILE="perl6_pass_rates"
OUTPUT=`./cull`
HEADER=`head -1 $PASSFILE`
echo "$HEADER" > $TEMPFILE
echo "$OUTPUT" >> $TEMPFILE
tail -$(wc -l $PASSFILE|awk '{print $1-1}') $PASSFILE >> $TEMPFILE
mv $TEMPFILE $PASSFILE
git commit -m "today" "$PASSFILE"
